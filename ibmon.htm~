		<!-- for initializing the length -->
		<script type="text/javascript">
			$(document).ready(function () {
				$.localScroll.defaults.axis = 'x';
				$.localScroll();
				updsize();
			});
		
			function updsize() {
				newWidth = $(window).width();
				newHeight = $(window).height();
				console.log(newWidth);
				$('.box').width(newWidth); 
				//$('.innerbox').width(newWidth * 4 / 5);
				$('.box').height(newHeight - 100);
				$('#subcon').width(newWidth*4 + 20);
			}
		
			$(window).resize(function() {
				updsize();
			});
		
		</script>
		
		<!-- initialize lightbox -->
		<script type="text/javascript">
		    $(function() {
		        function launch() {
		             $('#sign_up').lightbox_me({centered: true, onLoad: function() { $('#sign_up').find('input:first').focus()}});
		        }
		        
		        $('#q1').click(function(e) {
		            $("#a1").lightbox_me({centered: true});
		            e.preventDefault();
		        });
		        
		    });
		</script>

<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
<script>
	function initialize() {
	  var myLatlng = new google.maps.LatLng(-25.363882,131.044922);
	  var latlngMalang = new google.maps.LatLng(-7.969377,112.632713);
	  var mapOptions = {
		zoom: 4,
		center: myLatlng
	  }
	  
	  var mapOptionsMalang = {
		zoom: 14,
		center: latlngMalang
	  }
	  var mapjogja = new google.maps.Map(document.getElementById('mapjogja'), mapOptions);

	  var markerjogja = new google.maps.Marker({
		  position: myLatlng,
		  map: mapjogja,
		  title: 'Hotel Universitas'
	  });
	  
	  var mapmalang = new google.maps.Map(document.getElementById('mapmalang'), mapOptionsMalang);

	  var markermalang = new google.maps.Marker({
		  position: latlngMalang,
		  map: mapmalang,
		  title: 'Hotel Reagent'
	  });
	}

	google.maps.event.addDomListener(window, 'load', initialize);

</script>	
